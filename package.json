{
  "name": "@polito/students-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --active-arch-only --appIdSuffix=dev",
    "ios": "react-native run-ios --scheme='students dev' || echo 'If iOS startup fails, try running: npm run fixpilot'",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "prepare": "husky",
    "commit": "commit",
    "lint:check": "eslint --max-warnings=0 '**/*.{ts,tsx}'",
    "lint": "npm run lint:check -- --fix",
    "format:check": "prettier --check '**/*.{ts,tsx,md}'",
    "format": "prettier --write '**/*.{ts,tsx,md}'",
    "types:check": "tsc --noEmit",
    "check": "npm run lint:check && npm run format:check && npm run types:check",
    "pod-install": "cd ios && bundle exec pod install",
    "fixpilot": "node ios/fixpilot.js",
    "postinstall": "npm run fixpilot -- --skip-modules --skip-derived"
  },
